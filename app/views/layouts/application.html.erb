<!DOCTYPE html>
<html>
  <head>
    <link rel="preload" href="<%= stylesheet_path('application') %>" as="style" onload="this.rel='stylesheet'">
    <noscript><link rel="stylesheet" href="<%= stylesheet_path('application') %>"></noscript>

    <%= render 'layouts/prefetch' %>
    <%= render 'layouts/shim' %>

    <%= favicon_link_tag 'pin2.png' %>
    <%= csrf_meta_tags %>

    <%= render 'layouts/google_analytics' %>
    <%= render 'layouts/view_port' %>
    <title>Spherelink</title>
  </head>
  <body>
    <%= render 'layouts/flash' %>
    <%= render 'layouts/navbar' %>
    <%= yield %>
  </body>

  <%= javascript_include_tag 'application' %>
</html>
